{
    "require": {
        "mpdf/mpdf": "^8.0",
        "php-di/php-di": "^6.1",
        "selective/basepath": "^1.0",
        "selective/validation": "^0.10.0",
        "slim/psr7": "^1.1",
        "slim/slim": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
	    "psr-4": {
	        "App\\": "src/"
	    }
	},
	"autoload-dev": {
	    "psr-4": {
	        "App\\Test\\": "tests/"
	    }
	},
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
